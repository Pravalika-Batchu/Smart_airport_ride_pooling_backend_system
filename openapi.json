          },
          "luggage_count": {
            "type": "integer",
            "title": "Luggage Count",
            "default": 0
          },
          "pickup_time_window_start": {
            "type": "string",
            "format": "date-time",
            "title": "Pickup Time Window Start"
          },
          "pickup_time_window_end": {
            "type": "string",
            "format": "date-time",
            "title": "Pickup Time Window End"
