        "title": "RideRequestCreate"
      },
      "RideRequestResponse": {
        "properties": {
          "user_id": {
            "type": "string",
            "title": "User Id"
          },
          "pickup_lat": {
            "type": "number",
            "title": "Pickup Lat"
          },
          "pickup_lon": {
            "type": "number",
            "title": "Pickup Lon"
          },
          "dropoff_lat": {
            "type": "number",
            "title": "Dropoff Lat"
          },
          "dropoff_lon": {
            "type": "number",
            "title": "Dropoff Lon"
          },
          "seats_needed": {
            "type": "integer",
            "title": "Seats Needed",
            "default": 1
          },
          "luggage_count": {
            "type": "integer",
            "title": "Luggage Count",
            "default": 0
          },
          "pickup_time_window_start": {
            "type": "string",
            "format": "date-time",
            "title": "Pickup Time Window Start"
          },
          "pickup_time_window_end": {
            "type": "string",
            "format": "date-time",
            "title": "Pickup Time Window End"
          },
          "id": {
            "type": "string",
            "title": "Id"
          },
          "status": {
            "$ref": "#/components/schemas/RideStatus"
          },
          "request_time": {
            "type": "string",
            "format": "date-time",
            "title": "Request Time"
          },
          "trip_id": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Trip Id"
          },
          "estimated_fare": {
            "anyOf": [
              {
                "type": "number"
              },
              {
                "type": "null"
              }
            ],
