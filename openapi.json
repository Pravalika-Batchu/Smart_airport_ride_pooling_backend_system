        "required": [
          "user_id",
          "pickup_lat",
          "pickup_lon",
          "dropoff_lat",
          "dropoff_lon",
          "pickup_time_window_start",
          "pickup_time_window_end",
          "id",
          "status",
          "request_time"
        ],
        "title": "RideRequestResponse"
